  3.770670537342614*^9}},ExpressionUUID->"f5207e94-c291-43a4-bf07-\
ee22dec33697"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Product", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         RowBox[{"r", 
          RowBox[{"(", 
           RowBox[{"r", "+", "2"}], ")"}]}], ")"}]}]}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"Log", "[", 
       RowBox[{"2", ",", "r"}], "]"}], ")"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"r", ",", "1", ",", "Infinity"}], "}"}]}], "]"}], "//", 
  "N"}]], "Input",
