   RowBox[{"p", ",", "q"}], "}"}], "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Prime", "[", 
     RowBox[{"RandomInteger", "[", 
      RowBox[{"{", 
       RowBox[{"100", ",", "500"}], "}"}], "]"}], "]"}], ",", 
    RowBox[{"Prime", "[", 
     RowBox[{"RandomInteger", "[", 
      RowBox[{"{", 
       RowBox[{"10000", ",", "10400"}], "}"}], "]"}], "]"}]}], 
   "}"}]}], "\[IndentingNewLine]", 
 RowBox[{"publicModulus", "=", 
  RowBox[{"p", "*", "q"}]}]}], "Input",
